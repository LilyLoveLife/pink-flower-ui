{
  "name": "@pink-ui/components",
  "public": true,
  "version": "0.0.1",
  "main": "./dist/lib/index.umd.cjs",
  "module": "./dist/es/index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "lint-staged": "lint-staged",
    "dev": "vite",
    "build": "cross-env NODE_ENV=production pnpm build:components",
    "build:components": "esno ./scripts/esBuild.ts",
    "build:all": "tsc && vite build",
    "preview": "vite preview",
    "prettier": "prettier --write --ext .js,.jsx,.d.ts,.tsx,.json,.css,.scss,less src/",
    "eslint": "eslint --fix --ext .js,.jsx,.d.ts,.tsx,.json src/"
  },
  "dependencies": {
    "@rollup/plugin-typescript": "^11.1.1",
    "antd": "^5.4.7",
    "classnames": "^2.3.2",
    "fs-extra": "^11.1.1",
    "lodash": "^4.17.21",
    "only-allow": "^1.1.1",
    "path": "^0.12.7",
    "sass": "^1.62.1",
    "tslib": "^2.5.3"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.195",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^3.1.0",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.38.0",
    "eslint-config-pink": "^0.0.3",
    "esno": "^0.16.3",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.1",
    "rollup-plugin-visualizer": "^5.9.0",
    "typescript": "^4.9.3",
    "unocss": "^0.52.3",
    "vite": "^4.2.0",
    "husky": "^8.0.0"
  },
  "eslintConfig": {
    "extends": [
      "pink",
      "plugin:prettier/recommended"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,jsx,d.ts,tsx,json,css}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
